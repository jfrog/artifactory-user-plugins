{
  "policies": [{
    "cron": "0 0 1 ? * 1",
    "repos": [
      "libs-release-local"
    ],
    "timeUnit": "day",
    "timeInterval": 3,
    "dryRun": true,
    "paceTimeMS": 500,
    "disablePropertiesSupport": true,
    "keepArtifacts": true,
    "keepArtifactsRegex": "~/.*-[.\\d+]+-\\d{2}\\.[1-4][\\.\\w+]*/"
  }]
}
